                    self.aChoices.append( choice )
                    self.aDialogChoices += choice
                    self.aChoiceIndexes.append( index )
                index += 1
            self.dChoices = self.removeUnauthorizedCharacters(self.aDialogChoices)

    def onInput_onStop(self):
